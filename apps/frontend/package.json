{
  "name": "home",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "prestart": "cd ../../packages/types && npm run build",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:ci": "cross-env CI=true ng test --watch=false",
    "pretest": "cd ../../packages/types && npm run build",
    "test:watch": "ng test",
    "lint": "ng lint",
    "type-check": "ng build --configuration production",
    "test:e2e": "playwright test",
    "test:e2e:local": "cross-env BACKEND_PORT=3001 BACKEND_HOST=localhost FRONTEND_PORT=4201 USE_DOCKER_COMPOSE=true playwright test",
    "test:e2e:headed": "cross-env BACKEND_PORT=3001 BACKEND_HOST=localhost FRONTEND_PORT=4201 USE_DOCKER_COMPOSE=true playwright test --headed",
    "test:e2e:debug": "cross-env BACKEND_PORT=3001 BACKEND_HOST=localhost FRONTEND_PORT=4201 USE_DOCKER_COMPOSE=true playwright test --debug",
    "test:e2e:ui": "cross-env BACKEND_PORT=3001 BACKEND_HOST=localhost FRONTEND_PORT=4201 USE_DOCKER_COMPOSE=true playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:full": "npm run test:e2e:start && npm run test:e2e:wait && npm run test:e2e:local && npm run test:e2e:stop",
    "test:e2e:full:watch": "npm run test:e2e:start && npm run test:e2e:wait && npm run test:e2e:ui",
    "test:e2e:start": "docker-compose -f ../../docker-compose.e2e-local.yml up -d",
    "test:e2e:stop": "docker-compose -f ../../docker-compose.e2e-local.yml down",
    "test:e2e:restart": "npm run test:e2e:stop && npm run test:e2e:start",
    "test:e2e:wait": "node scripts/wait-for-services.js",
    "test:e2e:logs": "docker-compose -f ../../docker-compose.e2e-local.yml logs -f",
    "test:e2e:reset": "docker-compose -f ../../docker-compose.e2e-local.yml exec -T postgres-test psql -U postgres -d st44_test_local -f /docker-entrypoint-initdb.d/init.sql",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:docs": "compodoc -p tsconfig.json -e json -d ."
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.2",
    "@angular/router": "^21.0.0",
    "@st44/types": "file:../../packages/types",
    "date-fns": "^4.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^20.0.6",
    "@angular/compiler-cli": "^21.0.0",
    "@compodoc/compodoc": "^1.1.28",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/angular": "^10.1.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/bcrypt": "^6.0.0",
    "angular-eslint": "21.0.1",
    "bcrypt": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.1.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.3",
    "prettier": "^3.7.4",
    "storybook": "^10.1.10",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8"
  }
}
